{
    "notes": {
      "1": "Cases will reuse default, except for overrides",
      "2": "If no expected is provided, it will default to 'Add course successful' and a navigation to the add course page"
    },
    "default": {
      "CourseName": "Course 1",
      "Price": "100",
      "LinkThumbnail": "http://example.com/thumbnail.jpg",
      "Category": "Technology",
      "Description": "This is a description of Course 1",
      "expected": "Add course successful",
      "navigate": "http://localhost:3000/add-course/1"
    },
    "cases": [
      {
        "tc": "Single choice all valid data"
      },
      {
        "tc": "CourseName length = 0",
        "CourseName": "",
        "expected": "Course name is required"
      },
      {
        "tc": "CourseName length = 1",
        "CourseName": "a"
      },
      {
        "tc": "CourseName length = 2",
        "CourseName": "aa"
      },
      {
        "tc": "CourseName length = 100",
        "CourseName": "aaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaa"
      },
      {
        "tc": "CourseName length = 254",
        "CourseName": "aaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaa"
      },
      {
        "tc": "CourseName length = 255",
        "CourseName": "aaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaa"
      },
      {
        "tc": "CourseName length = 256",
        "CourseName": "aaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaa",
        "expected": "Fail to add course"
      },
      {
        "tc": "CourseName length = 400",
        "CourseName": "",
        "expected": "Fail to add course"
      },
      {
        "tc": "Price negative value",
        "Price": "-100",
        "expected": "Invalid price"
      },
      {
        "tc": "Price zero",
        "Price": "0",
        "expected": "Invalid price"
      },
      {
        "tc": "Price valid value",
        "Price": "200"
      },
      {
        "tc": "LinkThumbnail invalid URL",
        "LinkThumbnail": "invalid-url",
        "expected": "Invalid URL"
      },
      {
        "tc": "LinkThumbnail valid URL",
        "LinkThumbnail": "http://example.com/new-thumbnail.jpg"
      },
      {
        "tc": "Category length = 0",
        "Category": "",
        "expected": "Category is required"
      },
      {
        "tc": "Category valid value",
        "Category": "Science"
      },
      {
        "tc": "Description length = 0",
        "Description": "",
        "expected": "Description is required"
      },
      {
        "tc": "Description length = 1",
        "Description": "a"
      },
      {
        "tc": "Description length = 2",
        "Description": "aa"
      },
      {
        "tc": "Description length = 100",
        "Description": "aaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaa"
      },
      {
        "tc": "Description length = 254",
        "Description": "aaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaa"
      },
      {
        "tc": "Description length = 255",
        "Description": "aaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaa"
      },
      {
        "tc": "Description length = 256",
        "Description": "aaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaa",
        "expected": "Fail to add course"
      },
      {
        "tc": "Description length = 400",
        "Description": "",
        "expected": "Fail to add course"
      }
    ]
  }
  